<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:background="@drawable/cell_student">

	<TextView
		android:id="@+id/registerNumber"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="2dp"
		android:layout_marginTop="1dp"
		android:layout_marginEnd="2dp"
		android:text="TextView"
		android:textColor="#6E6A6A"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/nameStudent"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="2dp"
		android:layout_marginTop="5dp"
		android:layout_marginEnd="2dp"
		android:text="TextView"
		android:textColor="#040303"
		android:textSize="24sp"
		android:textStyle="bold"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@id/registerNumber" />

	<TextView
		android:id="@+id/career"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="2dp"
		android:layout_marginTop="5dp"
		android:layout_marginEnd="2dp"
		android:text="TextView"
		android:textAlignment="textEnd"
		android:textColor="#6E6A6A"
		android:textStyle="italic"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@id/nameStudent" />
</androidx.constraintlayout.widget.ConstraintLayout>
